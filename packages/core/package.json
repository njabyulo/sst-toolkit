{
  "name": "@sst-toolkit/core",
  "version": "1.0.0",
  "private": true,
  "description": "Core utilities for SST state parsing, relationship detection, and workflow building",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./state": {
      "types": "./dist/state/index.d.ts",
      "import": "./dist/state/index.mjs",
      "require": "./dist/state/index.js",
      "default": "./dist/state/index.mjs"
    },
    "./relationships": {
      "types": "./dist/relationships/index.d.ts",
      "import": "./dist/relationships/index.mjs",
      "require": "./dist/relationships/index.js",
      "default": "./dist/relationships/index.mjs"
    },
    "./workflow": {
      "types": "./dist/workflow/index.d.ts",
      "import": "./dist/workflow/index.mjs",
      "require": "./dist/workflow/index.js",
      "default": "./dist/workflow/index.mjs"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.js",
      "default": "./dist/adapters/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "pnpm lint:type && pnpm lint:code",
    "lint:code": "eslint .",
    "lint:type": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@sst-toolkit/shared": "workspace:*"
  },
  "peerDependencies": {
    "sst": "catalog:"
  },
  "devDependencies": {
    "@pulumi/pulumi": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "sst": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}



