{
  "name": "@sst-toolkit/cli",
  "version": "1.0.0",
  "private": true,
  "description": "CLI tool for SST Toolkit",
  "type": "module",
  "bin": {
    "sst-toolkit": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "lint": "pnpm lint:type && pnpm lint:code",
    "lint:code": "eslint .",
    "lint:type": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@sst-toolkit/core": "workspace:*",
    "@sst-toolkit/plugin-sdk": "workspace:*",
    "@sst-toolkit/shared": "workspace:*",
    "commander": "catalog:",
    "@aws-sdk/client-apigatewayv2": "catalog:",
    "@aws-sdk/client-cloudwatch-logs": "catalog:",
    "@aws-sdk/client-dynamodb": "catalog:",
    "@aws-sdk/client-ec2": "catalog:",
    "@aws-sdk/client-eventbridge": "catalog:",
    "@aws-sdk/client-lambda": "catalog:",
    "@aws-sdk/client-resource-groups-tagging-api": "catalog:",
    "@aws-sdk/client-s3": "catalog:",
    "@aws-sdk/client-cloudwatch": "catalog:",
    "@aws-sdk/client-elasticache": "catalog:",
    "@aws-sdk/client-iam": "catalog:",
    "@aws-sdk/client-rds": "catalog:",
    "@aws-sdk/client-servicediscovery": "catalog:",
    "@aws-sdk/client-sqs": "catalog:",
    "@aws-sdk/credential-providers": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}




